<h1>Admins Listing</h1>
<%= link_to '<button>Home</button>'.html_safe, controller: 'admin', action: 'index'  %>
<p id="notice"><%= notice %></p>
<table>
  <thead>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>E-Mail Address</th>
  </tr>
  </thead>

  <tbody>
  <% @admins.each do |admin| %>
      <% #if (admin.id != session[:admin_id])%>
          <tr>
            <td><%= admin.id %></td>
            <td><%= admin.name %></td>
            <td><%= admin.email %></td>
            <td><%= link_to 'Delete Administrator', admin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
      <% #end %>
  <% end %>
  </tbody>
</table>
<br>